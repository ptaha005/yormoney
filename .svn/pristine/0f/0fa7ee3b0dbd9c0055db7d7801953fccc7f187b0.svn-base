    padding-top: 10px;
}
.library {
    padding-bottom: 30px;
}
.library p{
    font-size: 13px;
}
.line {
    border-bottom: 1px solid #999999;
}
.videos {
    height: 250px;
    margin-top: 10px;
    overflow-y: scroll;
}
.videos img{
    margin-right: 6px;
    margin-bottom: 6px;
}
.banner_video {
    margin-top: 10px;
}

/*video library*/

/*help*/
.help   {
    padding-bottom: 20px;
}
.info_table {
   overflow: hidden;
    margin-bottom: 20px;
}
.border_bott {
    border-bottom: 1px solid #adadad;
    overflow: hidden;
}
.tabl {
    display: block;
    float: left;
    width: 30%;
    border-right: 1px solid #adadad;
    padding-left: 10px;
    padding-bottom: 3px;
    font-size: 14px;
}
.table .tabl {
    height: 270px;
}
.table textarea {
    border: 1px solid #adadad;
    border-radius: 8px;
    -webkit-box-shadow: inset 1px 8px 6px -6px #CAA3A3;
    -moz-box-shadow: inset 1px 8px 6px -6px #CAA3A3;
    box-shadow: inset 1px 8px 6px -6px #CAA3A3;
}
.tabl:first-child{
    padding-left: 0;
}
.tabl:last-child{
    border-right: none;
}
.tabl p {
    padding: 8px 0;
}
.tabl  .button_next {
    margin-top: 10px;
}
/*help*/

/*income page*/
.income_page {
    width: 98%;
    height: 547px;
    background: #f8f8f8;
    border-radius: 8px;
}

.income_top{
    display: block;
    float: left;
    width: 560px;
    height: 130px;
    background: url("../img/shadow_bottom.png") no-repeat scroll bottom center transparent;
}
.income_top .title {
    font-size: 43px;
    font-weight: bold;
    line-height: 40px;
}
.title_text {
    margin-left: 20px;
    margin-top: 20px;
}

.under_title {
    font-size: 19px;
    font-weight: bold;
}
.income_page .button {
    margin-top: 20px;
    margin-right: 20px;
}
.income_page .inputs .button_inp{
    padding: 0 10px;
    font-weight: bold;
    font-size: 20px;
    width: auto;
}
.fleft {
    float: left;
}
/*income page*/


#form_registration form .left_form .inputs input.input_field.invalid,
#form_registration form .right_form .inputs input.input_field.invalid
{
    border-color: red;
    box-shadow: 0 0 1px 1px red inset, 0 0 2px red;
    outline: 0 none;
}